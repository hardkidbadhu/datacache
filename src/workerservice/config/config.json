{
  "app_name": "data_cache_v1",
  "redis_vars": {
    "conn_string": "127.0.0.1:6379",
    "max_retries": 3,
    "min_retry_backoff": 120,
    "dial_timeout": 6,
    "read_timeout": 12,
    "write_timeout": 15
  },
  "database": {
    "type": "MongoDB",
    "name": "datacache",
    "uri": "mongodb://127.0.0.1:27017",
    "source": "admin",
    "timeout": 30,
    "pool_timeout": 12,
    "pool_conn_maxidle_time": 60000,
    "pool_limit": 100
  },
  "scheduler_db_backup": 1,
  "kafka": {
    "broker_address" : ["localhost:9093"],
    "topic": "restore_user"
  }
}